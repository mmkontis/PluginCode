{
  "manifest_version": 3,
  "name": "X.com GPT Reply",
  "version": "1.1",
  "key":"",
  "description": "Add GPT-powered reply suggestions to X.com posts",
  "permissions": [
    "activeTab",
    "scripting",
    "clipboardWrite",
    "storage",
    "identity",
    "tabs"
  ],
  
  "host_permissions": [
    "https://twitter.com/*",
    "https://x.com/*",
    "https://reply-bot-server.vercel.app/*",
    "https://replybot-server.vercel.app/*",
    "http://192.168.106.37:8080/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://x.com/*", "*://twitter.com/*","http://192.168.106.37:8080/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "web_accessible_resources": [
    {
      "resources": ["shadcn-components.js","popup.js"],
      "matches": ["*://x.com/*", "*://twitter.com/*","http://192.168.106.37:8080/*"],
      "type":"module"
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' ; object-src 'self'",
    "sandbox": "sandbox allow-scripts allow-forms allow-popups allow-modals; script-src 'self' 'unsafe-eval'; child-src 'self';"
  },
  "oauth2": {
    "client_id": "260056260511-dl8c70eq4rc0a8dj1ajm42arih978rq8.apps.googleusercontent.com.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  }
 

}